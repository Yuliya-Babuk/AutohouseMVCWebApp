@using AppAutohouse.PL.Models;

@model IEnumerable<RequestModel>

@{
    ViewData["Title"] = "Confirmed requests";

}
<div class="container">
    <p><h3>Confirmed requests</h3></p>
    <table class="table table-striped table-success table-bordered table-hover text-center align-middle">
        <thead>

            <tr class="table-warning">
                <th>    Name</th>
                <th>    Surname</th>
                <th>    Phone number</th>
                <th>    Car ID</th>

            </tr>

        <thead>

            @foreach (var item in @Model)
            {
                <tr>

                    <td>@item.Name</td>
                    <td>@item.Surname</td>
                    <td>@item.PhoneNumber</td>
                    <td>@item.CarId</td>

                </tr>

            }
    </table>
</div>

