@using MVCAppAutohouse.DAL.Entities;

@model Car

@{
    ViewData["Title"] = "Car info";
    Layout = "_Layout";
}

<table class="table table-striped table-success table table-hover table table-bordered">
    <thead>
        <tr class="table-warning">
            <th>    ID</th>
            <th>    Brand</th>
            <th>@nameof(Model.Model)</th>
            <th>    Engine Type</th>
            <th>    Engine Size, L</th>
            <th>@nameof(Model.Year)</th>
            <th>    Price, $</th>
        </tr>
    </thead>



    <tr>
        <td>@Model.Id</td>
        <td>@Model.Brand.Name</td>
        <td>@Model.Model</td>
        <td>@Model.TypeEngine</td>
        <td>@Model.EngineSize</td>
        <td>@Model.Year</td>
        <td>@Model.Price</td>
    </tr>
</table>

